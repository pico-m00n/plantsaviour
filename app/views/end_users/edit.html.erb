<h2>profile Edit</h2>
<%= form_with model: @end_user do |f| %>
     <div>
         <label for="inputName">Name</label><br>
         <%= f.text_field :name, autofocus: true, id:"inputName", placeholder:"名前"%>
     </div>
     <div>
     <label for="inputIntroduction">Introduction</label><br>
     <%= f.text_area :introduction, autofocus: true, id:"inputName", placeholder:"自己紹介"%>
     </div>
     <div>
     <label for="inputImage">ProfileImage</label><br>
     <%= f.file_field :profile_image, placeholder: "プロフィール画像", accept: "image/*" %>  
     </div>
     
     <%= f.submit "変更を保存" %>
<% end %>